{% extends 'backend.html.twig' %}

{% block title %}Contactusmessage{% endblock %}

{% block body %}
    <h1>Contactusmessage</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contactusmessage.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ contactusmessage.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ contactusmessage.email }}</td>
            </tr>
            <tr>
                <th>Subject</th>
                <td>{{ contactusmessage.subject }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ contactusmessage.message }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ contactusmessage.status ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a  class="btn btn-sm btn-default" href="{{ path('contactusmessage_index') }}">back to list</a>

    <a class="btn btn-sm btn-warning" href="{{ path('contactusmessage_edit', {'id': contactusmessage.id}) }}">edit</a>

    {{ include('contactusmessage/_delete_form.html.twig') }}
{% endblock %}
