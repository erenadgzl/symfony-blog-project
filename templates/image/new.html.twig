{% extends 'backend.html.twig' %}

{% block title %}New Image{% endblock %}

{% block body %}
    <h1>Create new Image</h1>

    <form action="{{ path('image_new') }}" method="post" enctype="multipart/form-data">
        Select image to upload:
        <input type="file" name="imagename" id="fileToUpload">
        <input type="submit" value="Upload Image" name="submit">
    </form>
    <a class="btn btn-sm btn-default" href="{{ path('image_index') }}">back to list</a>
{% endblock %}
